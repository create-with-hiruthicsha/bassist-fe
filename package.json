{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.2",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit -p tsconfig.app.json --incremental",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:component": "cypress run --component",
    "cypress:run:e2e": "cypress run --e2e",
    "cypress:test": "cypress run",
    "cypress:test:component": "cypress run --component",
    "cypress:test:e2e": "cypress run --e2e",
    "cypress:test:happy": "cypress run --spec \"cypress/e2e/happy-path/**/*.cy.ts\"",
    "cypress:test:sad": "cypress run --spec \"cypress/e2e/sad-path/**/*.cy.ts\"",
    "cypress:test:edge": "cypress run --spec \"cypress/e2e/edge-path/**/*.cy.ts\"",
    "cypress:test:alternative": "cypress run --spec \"cypress/e2e/alternative-path/**/*.cy.ts\"",
    "cypress:test:exception": "cypress run --spec \"cypress/e2e/exception-path/**/*.cy.ts\"",
    "cypress:test:negative": "cypress run --spec \"cypress/e2e/negative-path/**/*.cy.ts\"",
    "cypress:test:landing": "cypress run --spec \"cypress/e2e/**/landing-page.cy.ts\"",
    "cypress:test:task-generator": "cypress run --spec \"cypress/e2e/**/task-generator.cy.ts\"",
    "cypress:test:integrations": "cypress run --spec \"cypress/e2e/**/integrations.cy.ts\"",
    "cypress:test:document-generator": "cypress run --spec \"cypress/e2e/**/document-generator.cy.ts\"",
    "cypress:test:direct-create-tasks": "cypress run --spec \"cypress/e2e/**/direct-create-tasks.cy.ts\"",
    "cypress:test:oauth-result": "cypress run --spec \"cypress/e2e/**/oauth-result.cy.ts\"",
    "cypress:test:runner": "node cypress/scripts/run-tests.js",
    "cypress:test:help": "node cypress/scripts/run-tests.js --help",
    "version:patch": "npm version patch --no-git-tag-version",
    "prepare": "husky"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "npm run typecheck"
    ]
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.57.4",
    "@types/canvas-confetti": "^1.9.0",
    "canvas-confetti": "^1.9.4",
    "framer-motion": "^12.23.26",
    "lucide-react": "^0.344.0",
    "mammoth": "^1.11.0",
    "mermaid": "^11.12.0",
    "posthog-js": "^1.276.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.6.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.30.1"
  },
  "devDependencies": {
    "@cypress/react": "^9.0.1",
    "@eslint/js": "^9.9.1",
    "@tailwindcss/typography": "^0.5.19",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "cypress": "^15.9.0",
    "cypress-multi-reporters": "^1.6.4",
    "dotenv": "^17.2.3",
    "eslint": "^9.9.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.11",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "mocha": "^10.7.3",
    "mocha-junit-reporter": "^2.2.1",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.21"
  }
}
